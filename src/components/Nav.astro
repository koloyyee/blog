---
const routes = [
    { name: 'blog', link: '/' },
    { name: 'about', link: '/about/' },
    { name: 'tags', link: '/tags/' },
]
---

<ul class="nav-links">
    {
        routes.map((route) => (
            <a href={route.link}>
                <li> {route.name}</li>{' '}
            </a>
        ))
    }
</ul>
